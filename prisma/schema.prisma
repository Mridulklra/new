generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Bookmark {
  id        String   @id @default(uuid())
  userId    String   @map("user_id")
  url       String
  title     String
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@index([userId])
  @@map("bookmarks")
}
